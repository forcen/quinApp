{
  "name": "quin-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:css": "stylelint --cache --ignore-path .gitignore './**/*.scss'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint --cache --ignore-path .gitignore '**/*.js'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --check --ignore-path .gitignore '**/*.{js,scss,md}'",
    "prettier:fix": "prettier --write --ignore-path .gitignore '**/*.{js,scss,md}'"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.25.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.8.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.4.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
